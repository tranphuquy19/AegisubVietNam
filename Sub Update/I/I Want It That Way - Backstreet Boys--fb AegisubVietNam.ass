Dialogue: 0,0:00:25.91,0:00:31.38,hai,,0000,0000,0000,karaoke,Hãy tin những gì anh nói
Dialogue: 0,0:00:31.38,0:00:35.56,hai,,0000,0000,0000,karaoke,Anh muốn như thế
Dialogue: 0,0:00:35.56,0:00:42.76,hai,,0000,0000,0000,karaoke,Nhưng chúng ta là hai thế giới khác biệt
Dialogue: 0,0:00:42.76,0:00:47.67,hai,,0000,0000,0000,karaoke,Anh không thể chạm tới trái tim em
Dialogue: 0,0:00:47.67,0:00:50.46,hai,,0000,0000,0000,karaoke,Khi em nói
Dialogue: 0,0:00:50.46,0:00:54.97,hai,,0000,0000,0000,karaoke,Em muốn điều đó như vậy
Dialogue: 0,0:00:54.97,0:00:56.22,hai,,0000,0000,0000,karaoke,Hãy nói cho anh biết
Dialogue: 0,0:00:56.22,0:00:59.84,hai,,0000,0000,0000,karaoke,Điều đó không là gì hay chỉ là những rung động
Dialogue: 0,0:00:59.84,0:01:01.06,hai,,0000,0000,0000,karaoke,Nói với anh tại sao
Dialogue: 0,0:01:01.06,0:01:04.67,hai,,0000,0000,0000,karaoke,Điều đó không ý nghĩa gì hay chỉ là những lỗi lầm
Dialogue: 0,0:01:04.67,0:01:05.93,hai,,0000,0000,0000,karaoke,Nói với anh tại sao
Dialogue: 0,0:01:05.93,0:01:10.14,hai,,0000,0000,0000,karaoke,Khi anh không muốn nghe em nói rằng
Dialogue: 0,0:01:10.14,0:01:14.25,hai,,0000,0000,0000,karaoke,Em muốn điều đó như vậy.
Dialogue: 0,0:01:14.25,0:01:19.25,hai,,0000,0000,0000,karaoke,Anh có là sự khao khát
Dialogue: 0,0:01:19.25,0:01:23.80,hai,,0000,0000,0000,karaoke,Là người em ao ước không
Dialogue: 0,0:01:23.80,0:01:29.28,hai,,0000,0000,0000,karaoke,Phải, anh biết có lẽ đã quá trễ
Dialogue: 0,0:01:29.28,0:01:33.78,hai,,0000,0000,0000,karaoke,Nhưng anh muốn như vậy
Dialogue: 0,0:01:33.78,0:01:35.01,hai,,0000,0000,0000,karaoke,Hãy nói cho anh biết
Dialogue: 0,0:01:35.01,0:01:39.88,hai,,0000,0000,0000,karaoke,Điều đó không là gì hay chỉ là những rung động
Dialogue: 0,0:01:39.87,0:01:43.43,hai,,0000,0000,0000,karaoke,Điều đó không ý nghĩa gì hay chỉ là những lỗi lầm
Dialogue: 0,0:01:43.43,0:01:44.74,hai,,0000,0000,0000,karaoke,Hãy nói anh nghe
Dialogue: 0,0:01:44.74,0:01:48.99,hai,,0000,0000,0000,karaoke,Anh chẳng muốn nghe em nói
Dialogue: 0,0:01:48.99,0:01:53.81,hai,,0000,0000,0000,karaoke,Em muốn điều đó như thế
Dialogue: 0,0:01:53.81,0:01:58.40,hai,,0000,0000,0000,karaoke,Giờ đây anh có thể thấy mình đã đánh mất nhau
Dialogue: 0,0:01:58.40,0:02:03.23,hai,,0000,0000,0000,karaoke,Từ con đường chúng ta vẫn đồng hành
Dialogue: 0,0:02:03.23,0:02:05.65,hai,,0000,0000,0000,karaoke,Những dù khoảng cách bao nhiêu
Dialogue: 0,0:02:05.65,0:02:08.09,hai,,0000,0000,0000,karaoke,Anh vẫn muốn em biết rằng
